{
  "name": "mutual-de-servicios-al-policia/pvt",
  "description": "Plataforma Virtual de Tr√°mites",
  "keywords": [
    "pvt"
  ],
  "license": "GPL-3.0-only",
  "type": "project",
  "require": {
    "php": "^7.2.5",
    "adldap2/adldap2": "^10.2",
    "barryvdh/laravel-snappy": "^0.4.7",
    "doctrine/dbal": "^2.10.1",
    "fico7489/laravel-pivot": "^3.0",
    "fideloper/proxy": "^4.3",
    "fruitcake/laravel-cors": "^1.0",
    "h4cc/wkhtmltoimage-amd64": "^0.12.4",
    "h4cc/wkhtmltopdf-amd64": "^0.12.4",
    "laravel/framework": "^7.3",
    "laravel/tinker": "^2.4",
    "milon/barcode": "^7.0",
    "rap2hpoutre/laravel-log-viewer": "^1.4",
    "santigarcor/laratrust": "^5.2",
    "tymon/jwt-auth": "^2",
    "waavi/sanitizer": "^1.0"
  },
  "require-dev": {
    "mpociot/laravel-apidoc-generator": "^4.5.1"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate",
      "@php artisan jwt:secret -f"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "amend-cmd": [
      "@php artisan migrate:rollback"
    ],
    "pre-install-cmd": [
      "COMPOSER_PROCESS_TIMEOUT=600 ./version-switch.sh"
    ],
    "post-install-cmd": [
      "COMPOSER_PROCESS_TIMEOUT=600 ./post-install.sh"
    ],
    "clear": [
      "COMPOSER_PROCESS_TIMEOUT=600 ./composer-clear.sh"
    ],
    "version-seeder": [
      "COMPOSER_PROCESS_TIMEOUT=600 ./version-seeder.sh"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
